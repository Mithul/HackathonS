<%= simple_form_for(@trackee) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :last_login %>
    <%= f.input :login_count %>
    <%= f.input :trackBrowser %>
    <%= f.input :track %>
    <%= f.input :flag %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
